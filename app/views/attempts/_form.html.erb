<%= form_with(model: attempt, local: true) do |form| %>
  <% if attempt.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(attempt.errors.count, "error") %> prohibited this attempt from being saved:</h2>

      <ul>
      <% attempt.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field" style="display: none">
    <%= form.label :qid, class: "label" %>
    <%= form.number_field :qid, id: :attempt_qid ,class: "w3-input"%>
  </div>

  <div class="field">
    <%= form.label :uname, class: "label" %>
    <%= form.text_field :uname, id: :attempt_uname ,class: "w3-input" %>
  </div>

  <div class="field">
    <%= form.label :gname, class: "label" %>
    <%= form.text_field :gname, id: :attempt_gname ,class: "w3-input" %>
  </div>

  <div class="field">
    <%= form.label :sname, class: "label" %>
    <%= form.text_field :sname, id: :attempt_sname ,class: "w3-input" %>
  </div>

  <div class="field">
    <%= form.label :score, class: "label" %>
    <%= form.number_field :score, id: :attempt_score ,class: "w3-input" %>
  </div>

  <div class="field">
    <%= form.label :attempt, class: "label" %>
    <%= form.number_field :attempt, id: :attempt_attempt ,class: "w3-input" %>
  </div>

  <div class="field">
    <%= form.label :correct, class: "label" %>
    <%= form.number_field :correct, id: :attempt_correct ,class: "w3-input" %>
  </div>
<br>
  <div class="actions">
    <%= form.submit class: "w3-btn w3-teal"%>
  </div>
  <br>
<% end %>